{
  "name": "quick-rag",
  "version": "0.7.1",
  "description": "⚡ Quick RAG - Fast local AI with official Ollama & LM Studio SDK integration",
  "main": "src/index.node.js",
  "browser": "src/index.browser.js",
  "type": "module",
  "exports": {
    ".": {
      "node": "./src/index.node.js",
      "browser": "./src/index.browser.js",
      "import": "./src/index.js",
      "default": "./src/index.node.js"
    },
    "./react": {
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./test/run-tests.js",
    "start": "node ./example/run.js",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "rag",
    "quick-rag",
    "ollama",
    "lm-studio",
    "react",
    "local-ai",
    "vector-search",
    "retrieval-augmented-generation",
    "embeddings"
  ],
  "author": "Cihat Emre Karataş",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emredeveloper/rag-js-local.git"
  },
  "homepage": "https://github.com/emredeveloper/rag-js-local#readme",
  "bugs": {
    "url": "https://github.com/emredeveloper/rag-js-local/issues"
  },
  "files": [
    "src/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ollama": "^0.6.2",
    "@lmstudio/sdk": "^1.5.0"
  },
  "optionalDependencies": {
    "node-fetch": "^3.3.2"
  }
}
