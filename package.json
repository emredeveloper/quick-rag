{
  "name": "quick-rag",
  "version": "2.1.0",
  "description": "⚡ Quick RAG v2.0 - Production-ready RAG framework with Official Ollama & LM Studio SDKs, Document Loaders, Smart Chunking, Query Explainability, Dynamic Prompts, Decision Engine, Streaming, React Hooks, and more!",
  "main": "src/index.node.js",
  "browser": "src/index.browser.js",
  "types": "src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "node": "./src/index.node.js",
      "browser": "./src/index.browser.js",
      "import": "./src/index.js",
      "default": "./src/index.node.js"
    },
    "./react": {
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "scripts": {
    "test": "node ./test/run-tests.js",
    "test:all": "node ./test/run-tests.js && node ./test/run-integration.js",
    "test:integration": "node ./test/run-integration.js",
    "start": "node ./example/run.js",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "rag",
    "quick-rag",
    "ollama",
    "lm-studio",
    "react",
    "local-ai",
    "vector-search",
    "retrieval-augmented-generation",
    "embeddings",
    "pdf",
    "document-loader",
    "pdf-parser",
    "word",
    "excel",
    "decision-engine",
    "query-explainability",
    "prompt-management",
    "streaming",
    "typescript",
    "vector-store",
    "semantic-search",
    "ai",
    "llm",
    "chatbot",
    "knowledge-base"
  ],
  "author": "Cihat Emre Karataş",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emredeveloper/quick-rag.git"
  },
  "homepage": "https://github.com/emredeveloper/quick-rag#readme",
  "bugs": {
    "url": "https://github.com/emredeveloper/quick-rag/issues"
  },
  "files": [
    "src/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@lmstudio/sdk": "^1.5.0",
    "ollama": "^0.6.2",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.5"
  },
  "optionalDependencies": {
    "better-sqlite3": "^11.10.0",
    "mammoth": "^1.11.0",
    "node-fetch": "^3.3.2",
    "officeparser": "^5.2.1",
    "pdf-parse": "^1.1.4",
    "xlsx": "^0.18.5"
  }
}